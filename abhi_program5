using System;
namespace even
{
class program4
{
static void Main(string[]args)
{
   Console.WriteLine("Please enter number");
   int a = Convert.ToInt32(Console.ReadLine());
   if (a % 2 == 0)
   {
       Console.WriteLine("The number is even");
   }
   else
   {
       Console.WriteLine("The Number is odd");
Console.ReadLine();
}
}
}
