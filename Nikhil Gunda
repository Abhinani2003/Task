using System;

namespace asciii
{
    internal class Program
    {
        static void Main(string[] arg)
        { 
          char c= 'a';
            char x = 'A';
            char e = ' ';
            int y = c; int z = x; int f = e;

            Console.WriteLine(y);
            Console.WriteLine(z);
            Console.WriteLine(f);
            Console.ReadLine();
        }
    }
}
